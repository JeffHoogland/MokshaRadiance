            rel1.offset: 0 2; \
         } \
      } \
      OVER(_NAME"_bt_base",_REL,"default","prelight")	\
      OVER(_NAME"_bt_base_SH",_REL,"default","prelight")	\
      program { \
         signal: "mouse,down,1*"; source: _REL; \
         action: STATE_SET "active" 0.0; \
         target: _NAME"_bt_base"; \
         target: _NAME"_bt_base_SH"; \
         target: _NAME"_icon"; \
      } \
      program { \
         signal: "mouse,up,1*"; source: _REL; \
