            rel2.relative: 1.0 1.0;
            rel2.offset: 1 1;
            color_class: "fileman_base";
            FIXED_SIZE(150, 28)
         }
      }
      part { name: "bg";
		clip_to: "typebuf_clip";
         description { state: "default" 0.0;
            STAY_ON("inset")
            image.normal: "img/entry_bg.png";
            image.border: 6 6 6 6;
         }
      }
      part { name: "typebuf_label_clip"; type: RECT;
		clip_to: "typebuf_clip";
         description { state: "default" 0.0;
            STAY_ON("bg")
         }
      }
      part { name: "e.text.typebuf_label"; type: TEXT; mouse_events: 0;
         clip_to: "typebuf_label_clip";
         effect: SHADOW BOTTOM;
         scale: 1;
         description { state: "default" 0.0;
         	STAY_ON("bg")
         	rel1.offset: 5 0;
         	rel2.offset: -4 0;
            color: 255 255 255 255;
            color3: 255 255 255 255;
            color_class: "entry_text";
            text { font: "Sans"; size: 12;
               min: 0 1;
               align: 0.0 0.5;
               text_class: "fileman_typebuf";
            }
         }
      }
      part { name: "inset"; mouse_events: 0;
		clip_to: "typebuf_clip";
         description { state: "default" 0.0;
            image.normal: "img/entry1.png";
            image.border: 6 6 6 6;
            rel1.to: "base";
            rel2.to: "base";
            rel1.offset: 3 3;
            rel2.offset: -4 -4;
            max: 9999 21;
          }
         description { state: "focused" 0.0;
			inherit: "default" 0.0;
			image.normal: "img/entry2.png";
			color_class: "focus";
		  }
		 description { state: "disabled" 0.0;
			inherit: "default" 0.0;
			image.normal: "img/entry0.png";
		 }
      }
      part { name: "base_bvl"; mouse_events: 0;
		clip_to: "typebuf_clip";
		description { state: "default" 0.0;
			STAY_ON("base")
			image.normal: "img/scroll_shadow.png";
			image.border: 2 2 2 2;
			image.middle: 0;
		}
	  }
   }
   programs {
      program { name: "spin";
         signal: "e,state,busy,start"; source: "e";
         action: STATE_SET "spin" 0.0;
         transition: LINEAR 2.0;
         target: "logo";
         after: "spin2";
      }
      program { name: "spin2";
         action: STATE_SET "default" 0.0;
         target: "logo";
         after: "spin";
      }
      program {
         signal: "e,state,busy,start"; source: "e";
         action: STATE_SET "active" 0.0;
         transition: SINUSOIDAL 0.25;
         target: "busy_clip";
      }
      program {
         signal: "e,state,busy,stop"; source: "e";
         action: STATE_SET "default" 0.0;
         transition: SINUSOIDAL 1.0;
         target: "busy_clip";
         after: "spin3";
      }
      program { name: "spin3";
         action: ACTION_STOP;
         target: "spin";
         target: "spin2";
         after: "spin4";
      }
      program { name: "spin4";
         action: STATE_SET "default" 0.0;
         target: "logo";
      }
      program {
         signal: "e,state,typebuf,start"; source: "e";
         action: STATE_SET "active" 0.0;
         transition: SINUSOIDAL 0.25;
         target: "typebuf_clip";
      }
      program {
         signal: "e,state,typebuf,stop"; source: "e";
         action: STATE_SET "default" 0.0;
         transition: SINUSOIDAL 1.0;
         target: "typebuf_clip";
      }
   }
}

