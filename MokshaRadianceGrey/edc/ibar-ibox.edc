group { name: "e/modules/ibar/menu";

   parts {
      part { name: "sizer"; type: SPACER;
         description { state: "default"; }
         description { state: "hidden";
            rel1.relative: 0.5 1.0;
            rel2.relative: 0.5 1.0;
            minmul: 0 0;
         }
      }
      part { name: "clip"; type: RECT;
         description { state: "default";
            rel1.offset: -9999 -9999;
            rel2.offset: 9999 9999;
         }
         description { state: "hidden";
            inherit: "default";
            color: 0 0 0 0;
         }
      }
      part { name: "bg"; type: RECT; mouse_events: 0;
		clip_to: "clip";
         description { state: "default" 0.0;
            color_class: "menu_base";
            STAY_ON("sizer")
         }
      }
      part { name: "bg_SH"; mouse_events: 0;
		clip_to: "clip";
         description { state: "default" 0.0;
            image.normal: "img/menu_bg_SH.png";
            STAY_ON("bg")
            image.border: 2 2 2 2;
            fill.smooth: 0;
         }
      }
      part { name: "e.box"; type: BOX;
         clip_to: "clip";
         description { state: "default";
            rel1.to: "sizer";
            rel1.offset: 0 1;
            rel2.to: "sizer";
            rel2.offset: -1 -1;
            box { layout: "vertical";
               padding: 0 0;
               align: 0.5 0.5;
               min: 1 1;
            }
         }
      }
   }
   programs {
      program { signal: "e,state,hidden"; source: "e";
         action: STATE_SET "hidden" 0.0;
         target: "sizer";
         target: "clip";
      }
      
      program { signal: "e,action,show"; source: "e";
         action: STATE_SET "default" 0.0;
         target: "sizer";
         target: "clip";
         transition: LINEAR 0.0;
         after: "show2";
      }
      program { name: "show2";
         action: SIGNAL_EMIT "e,action,show,done" "";
      }
      
      program { signal: "e,action,hide"; source: "e";
         action: STATE_SET "hidden" 0.0;
         target: "sizer";
         target: "clip";
         transition: LINEAR 0.0;
         after: "hide2";
      }
      program { name: "hide2";
        // in: 0.2 0.0;
         action: SIGNAL_EMIT "e,action,hide,done" "";
      }
   }
}

group { name: "e/modules/ibar/menu/item";

   parts {
      part { name: "icon"; type: SPACER;
         description { state: "default";
            rel1.offset: 6 2;
            rel2.offset: 2 -3;
            align: 0.0 0.5;
            aspect: 1.0 1.0; aspect_preference: BOTH;
            min: 48 48;
            max: 48 48;
         }
      }
      part { name: "base"; mouse_events: 0; type: RECT;
         description { state: "default" 0.0;
            color_class: "focus";
            visible: 0;
         }
         description { state: "focus" 0.0;
            inherit: "default" 0.0;
            visible: 1;
         }
      }
      part { name: "base_SH"; type: IMAGE; mouse_events: 0;
		  description { state: "default" 0.0;
			STAY_ON("base")
			  image.normal: "img/menu_sel_SH.png";
			  image.border: 2 2 2 2;
			  visible: 0;
		  }
		  description { state: "focus" 0.0;
            inherit: "default" 0.0;
            visible: 1;
         }
	  }
	  part { name: "icon_shadow"; mouse_events: 0;
		clip_to: "icon_clip";
		description { state: "default" 0.0;
			image.normal: "win_glow.png";
			image.border: 9 9 9 9;
            image.middle: 0;
			color: 0 0 0 255;
			rel1.to: "e.swallow.icon";
			rel2.to: "e.swallow.icon";
			rel1.offset: -4 -4;
			rel2.offset: 3 3;
		}
	  }
	  part { name: "e.swallow.icon"; type: SWALLOW; mouse_events: 0;
         clip_to: "icon_clip";
         description { state: "default";
            STAY_ON("icon")
         }
      }
      part { name: "e.text.title"; type: TEXT; mouse_events: 0;
         effect: SHADOW TOP;
         scale: 1;
         description { state: "default" 0.0;
            rel1.to_x: "icon";
            rel1.relative: 1.0 0.0;
            rel1.offset: 6 0;
            rel2.offset: -3 -3;
            color: 255 255 255 255;
            color3: 255 255 255 255;
            align: 0.0 0.0;
            color_class: "menu_item";
            min: 128 0;
            text { font: FNBD; size: 12;
               text: "window name thingy";
               min: 0 1;
               align: 0.0 0.5;
               text_class: "menu_item";
            }
         }
         description { state: "focus";
            inherit: "default";
            color_class: "menu_item_active";
         }
      }
      part { name: "icon_clip"; type: RECT;
         description { state: "default";
            color: 255 255 255 255;
            rel1.offset: -9999 -9999;
            rel2.offset: 9999 9999;
         }
      }
      part { name: "event"; type: RECT;
         description { state: "default";
            color: 0 0 0 0;
         }
      }
   }
   programs {
      program {
         signal: "mouse,in"; source: "event";
         action: STATE_SET "focus" 0.0;
         target: "e.text.title";
         target: "base";
         target: "base_SH";
         transition: LINEAR 0.0;
      }
      program {
         signal: "mouse,out"; source: "event";
         action: STATE_SET "default" 0.0;
         target: "e.text.title";
         target: "base";
         target: "base_SH";
         transition: LINEAR 0.0;
      }
   }
}
