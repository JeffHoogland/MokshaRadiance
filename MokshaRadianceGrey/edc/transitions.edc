group { name: "e/transpreview/0";
   images.image: "img_example_1.png" LOSSY 92;
   parts {
      part { name: "clip"; type: RECT;
         description { state: "default" 0.0;
         }
      }
      part { name: "bg";
         clip_to: "clip";
         description {state: "default" 0.0;
            image.normal: "img_example_1.png";
            ASPECT_FILL((400/300))
         }
      }
   }
}

